<?xml version="1.0" encoding="UTF-8" ?>
<!--MyBatis配置文件-->
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.jx.blogap1.dao.mapper.ClockInMapper">

    <insert id="insertClockIn" parameterType="com.jx.blogap1.dao.pojo.ClockIn">
        INSERT INTO ms_clock_in (id,content, create_date, user_id) VALUES (null ,#{content},#{createDate},#{userId})
    </insert>

    <select id="getOneDayPlan" resultType="java.lang.String" parameterType="long">
        SELECT content FROM ms_clock_in where create_date &gt;= #{oneDayZeroTime} AND create_date  &lt;= #{oneDayZeroTime} +86400000
    </select>

